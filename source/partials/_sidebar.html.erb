  <input type="radio" id="cs-sidebar-nav-appearance" name="cs-sidebar-nav" checked="checked">
  <label for="cs-sidebar-nav-appearance" title="Editar aparência" class="cs-tab-label cs-ico-apparence">aparência</label>
  <input type="radio" id="cs-sidebar-nav-pages" name="cs-sidebar-nav" >
